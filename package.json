{
  "name": "@navtrack/navtrack-shared",
  "version": "1.0.4",
  "description": "Shared library for the Navtrack web and mobile apps.",
  "author": {
    "name": "CodeAgency",
    "email": "support@navtrack.com"
  },
  "main": "dist/index.js",
  "scripts": {
    "build": "npx tsc --build",
    "build:watch": "npx tsc --build --watch",
    "sync": "rm -rf ../navtrack-mobile/node_modules/@navtrack/navtrack-shared/dist/* && rm -rf ../navtrack/Navtrack.Web/ClientApp/node_modules/@navtrack/navtrack-shared/dist/* && cp -r ./dist/ ../navtrack-mobile/node_modules/@navtrack/navtrack-shared/dist/ && cp -r ./dist/ ../navtrack/Navtrack.Web/ClientApp/node_modules/@navtrack/navtrack-shared/dist/",
    "sync:watch": "watch 'npm run sync' ./dist",
    "build:sync": "concurrently \"npm run build:watch\" \"npm run sync:watch\"",
    "generate": "orval --config ./orval.config.js"
  },
  "prepublish": "tsc",
  "devDependencies": {
    "@types/react": "^17.0.38",
    "concurrently": "^7.0.0",
    "orval": "^6.5.3",
    "typescript": "^4.5.5",
    "watch": "^1.0.2"
  },
  "peerDependencies": {
    "axios": "^0.25.0",
    "react": "^17.0.2",
    "react-query": "^3.34.12",
    "recoil": "^0.6.1"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/navtrack/navtrack-shared.git"
  },
  "typings": "dist/index.d.ts",
  "publishConfig": {
    "access": "public"
  }
}